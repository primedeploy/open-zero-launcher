{
  "expo": {
    "name": "Open Zero Launcher",
    "slug": "open-zero-launcher",
    "scheme": "open-zero-launcher",
    "version": "1.0.0",
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Open Zero Launcher to use your location to show weather information."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000",
          "imageWidth": 200,
          "dark": {
            "image": "./src/assets/images/icon.png",
            "backgroundColor": "#000"
          }
        }
      ],
      "expo-asset"
    ],
    "android": {
      "newArchEnabled": true,
      "package": "com.primedeploy.openzerolauncher",
      "versionCode":1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.QUERY_ALL_PACKAGES",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.REQUEST_DELETE_PACKAGES"
      ],
      "intentFilters": [
        {
          "action": "android.intent.action.MAIN",
          "category": [
            "android.intent.category.HOME",
            "android.intent.category.DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ef36e28d-0cd6-4935-9223-d600649f72c4"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/ef36e28d-0cd6-4935-9223-d600649f72c4"
    },
    "ios": {
      "bundleIdentifier": "com.primedeploy.openzerolauncher"
    }
  }
}
